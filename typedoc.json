{
  "name": "App Name",
  "mode": "modules",
  "out": "docs",
  "theme": "default",
  "ignoreCompilerErrors": "false",
  "preserveConstEnums": "true",
  "excludeExternals": true,
  "exclude": [
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/.templates/**"
  ],
  "external-modulemap": ".*packages/([^/]+)/.*",
  "stripInternal": "false"
}
