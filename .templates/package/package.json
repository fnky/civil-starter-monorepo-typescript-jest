{
  "name": "react-filecore",
  "version": "0.1.1",
  "description": "React adapter layer for React",
  "main": "common/index.js",
  "module": "module/index.js",
  "types": "types/index.d.js",
  "author": "Nicholas Dobie <nicholas.dobie@target.com>",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://binrepo.target.com/artifactory/api/npm/npm-pdex/"
  },
  "scripts": {
    "preinstall": "npx use-yarn",
    "build:common": "tsc --project . --module commonjs --outDir common/ --sourceMap",
    "build:module": "tsc --project . --moduleResolution Node --module es6 --outDir module/ --sourceMap",
    "build:types": "tsc --project . --module commonjs --outDir types/ --declaration --emitDeclarationOnly",
    "build": "run-p build:*",
    "watch:common": "tsc --watch --project . --module commonjs --outDir common/ --sourceMap",
    "watch:module": "tsc --watch --project . --moduleResolution Node --module es6 --outDir module/ --sourceMap",
    "watch:types": "tsc --watch --project . --module commonjs --outDir types/ --declaration --emitDeclarationOnly",
    "watch": "run-p watch:*",
    "prerelease": "run-p build:*",
    "start": "run-p watch:*",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "filecore-common": "^0.1.0",
    "filecore-manager": "^0.1.0"
  },
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.6.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.1",
    "@types/lolex": "^2.1.3",
    "@types/react": "^16.4.18",
    "@types/uuid": "^3.4.4",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "jest": "^23.5.0",
    "jest-enzyme": "^7.0.0",
    "lolex": "^2.7.5",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1",
    "use-yarn": "^2.2.0"
  }
}
